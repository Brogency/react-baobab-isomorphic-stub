{
  "name": "react-baobab-isomporhic-stub",
  "description": "Isomorphic starterkit with server-side React + Baobab rendering.",
  "version": "0.0.2",
  "license": "MIT",
  "main": "dist/server.js",
  "scripts": {
    "test": "karma start configs/karma.js",
    "start": "forever --minUptime 1000 --spinSleepTime 1000 -c \"node --harmony\" ./dist/server.js",
    "build-server": "webpack --colors --display-error-details --config configs/webpack.server.js",
    "build-client": "webpack --colors --display-error-details --config configs/webpack.client.js",
    "build": "concurrently \"npm run build-server\" \"npm run build-client\"",
    "watch-server": "webpack --watch --verbose --colors --display-error-details --config configs/webpack.server-watch.js",
    "watch-server-start": "just-wait --pattern \"dist/*.js\" && npm run start",
    "watch-client": "webpack-dev-server --config configs/webpack.client-watch.js",
    "watch": "concurrently --kill-others \"npm run watch-server-start\" \"npm run watch-server\" \"npm run watch-client\""
  },
  "dependencies": {
    "axios": "^0.11.0",
    "babel-polyfill": "6.5.0",
    "baobab": "^2.3.3",
    "baobab-prop-types": "^1.0.0",
    "baobab-react": "^1.0.1",
    "baobab-react-mixins": "^1.0.0",
    "baobab-react-resolver": "^1.0.1",
    "baobab-react-schemabranchmixin": "^1.0.9",
    "extract-text-webpack-plugin": "^1.0.1",
    "fetch-plus": "3.6.1",
    "fetch-plus-bearerauth": "3.5.0",
    "fetch-plus-json": "3.6.0",
    "file-loader": "0.8.5",
    "isomorphic-fetch": "2.2.1",
    "koa": "~2.0.0",
    "koa-convert": "~1.2.0",
    "koa-proxy": "~0.4.1",
    "koa-static": "~2.0.0",
    "lodash": "^4.6.1",
    "nconf": "~0.8.4",
    "react": "0.14.7",
    "react-dom": "0.14.7",
    "react-router": "2.0.0",
    "redbox-react": "^1.2.3",
    "transform-keys": "0.0.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.15"
  },
  "devDependencies": {
    "autoprefixer": "6.3.6",
    "autoprefixer-loader": "^3.2.0",
    "babel": "6.5.2",
    "babel-core": "6.5.2",
    "babel-loader": "6.2.3",
    "babel-preset-es2015": "6.5.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "chai": "^3.4.0",
    "concurrently": "2.0.0",
    "css-loader": "^0.23.1",
    "fake-style-loader": "1.0.1",
    "forever": "0.15.1",
    "ignore-loader": "^0.1.1",
    "isparta-loader": "^2.0.0",
    "json-loader": "0.5.4",
    "just-wait": "1.0.5",
    "karma": "^0.13.14",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^0.5.3",
    "karma-mocha": "^0.2.0",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-phantomjs-shim": "^1.1.2",
    "karma-sinon": "^1.0.4",
    "karma-sinon-chai": "^1.1.0",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.3",
    "phantomjs": "^1.9.17",
    "postcss-csso": "1.1.2",
    "postcss-loader": "0.9.1",
    "postcss-nested": "1.0.0",
    "react-hot-loader": "1.3.0",
    "postcss-reporter": "1.3.3",
    "react-addons-test-utils": "0.14.7",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.0",
    "stylelint": "^6.3.3",
    "stylelint-config-bro": "git+https://git@github.com/Brogency/stylelint-config-bro.git",
    "webpack-dev-server": "1.14.1",
    "webpack-node-externals": "0.4.1"
  },
  "engines": {
    "node": ">=4.0.0"
  }
}
